	     {"traceValue": "rd_addr", "description": "${${BITFIELD rd_addr [(0:11,7)]}}"},
	     {"traceValue": "rs1_data", "description": "*((RV32IMACFD*)cpu)->X[${ ${BITFIELD rs1_data [(0:19,15)]} }]"},
