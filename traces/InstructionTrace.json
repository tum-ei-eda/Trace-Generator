	     {"traceValue": "rs2_addr", "description": "${${BITFIELD rs2 [(0:24,20)]}}"},
	     {"traceValue": "imm", "description": "${${BITFIELD imm [(12:31,31)|(11:7,7)|(5:30,25)|(1:11,8)]}}"}
	 ]
	}
    ]
}}
